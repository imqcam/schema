{
  "title": "Archival ULI Build (simplified)",
  "description": "Schema for Archival ULI Builds, only selected fields are included",
  "type": "object",
  "properties": {
    "buildId": {
      "type": "string",
      "title": "Build ID",
      "propertyOrder": 1
    },
    "location": {
      "type": "string",
      "const": "CMU",
      "title": "Location",
      "default": "CMU",
      "propertyOrder": 2
    },
    "projectName": {
      "type": "string",
      "title": "Project Name",
      "default": "NASAULI",
      "propertyOrder": 3
    },
    "scanPower": {
      "type": "number",
      "title": "Scan Power (W)",
      "propertyOrder": 4
    },
    "scanVelocity": {
      "type": "number",
      "title": "Scan velocity (mm/s)",
      "propertyOrder": 5
    },
    "hatchSpacing": {
      "type": "number",
      "title": "Hatch Spacing (mm)",
      "propertyOrder": 6
    },
    "b": {
      "type": "number",
      "title": "breadth (mm)",
      "description": "Breadth of the build in mm",
      "propertyOrder": 7
    },
    "d": {
      "type": "number",
      "title": "depth (mm)",
      "description": "Depth of the build in mm",
      "propertyOrder": 8
    },
    "l": {
      "type": "number",
      "title": "Length (mm)",
      "description": "Length of the build in mm",
      "propertyOrder": 9
    }
  },
  "required": [
    "buildId",
    "location",
    "projectName",
    "scanPower",
    "scanVelocity",
    "hatchSpacing"
  ]
}
